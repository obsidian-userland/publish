{
  "name": "@obsidian-userland/publish",
  "version": "1.0.3",
  "files": [
    "dist"
  ],
  "targets": {
    "default": {
      "distDir": "./output"
    },
    "frontend": {
      "source": "index.html"
    },
    "backend": {
      "source": "src/index.js"
    }
  },
  "scripts": {
    "dev": "npm run clean && parcel build index.html --no-source-maps --no-content-hash",
    "build": "npm run clean && parcel build src/index.js --no-source-maps --no-content-hash",
    "clean": "rm -rf output",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.9",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "framer-motion": "^3.2.2-rc.1",
    "object-assign": "^4.1.1",
    "promise.series": "^0.2.0",
    "query-string": "^6.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-router": "^6.22.3",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "remark": "^15.0.1",
    "remark-gfm": "^1.0.0",
    "remark-wiki-link": "^1.0.4",
    "scheduler": "^0.23.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "parcel": "^2.0.0",
    "process": "^0.11.10",
    "typescript": "^4.1.3"
  }
}
